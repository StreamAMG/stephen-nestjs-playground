{
  "name": "nestjs-lambda-template",
  "version": "1.1.1",
  "description": "Template for NestJS API projects.",
  "private": true,
  "main": "src/main.js",
  "autor": "StreamAMG ZEUS team",
  "keywords": [
    "cloudpay-api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StreamAMG/nestjs-lambda-template.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StreamAMG/nestjs-lambda-template/issues"
  },
  "homepage": "https://github.com/StreamAMG/nestjs-lambda-template#readme",
  "scripts": {
    "clean:pre": "rimraf .build .cache .eslintcache .serverless coverage",
    "clean:post": "rm -rf .serverless/*.json",
    "prebuild": "rimraf dist",
    "package": "npm run clean:pre && serverless package && npm run clean:post",
    "format:check": "prettier --check \"./**/*.{ts,js}\"",
    "format": "prettier --write \"./**/*.{ts,js}\"",
    "lint": "eslint . --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:watch": "jest --runInBand --detectOpenHandles --watch --coverage=false",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --runInBand --detectOpenHandles",
    "start": "sls offline"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.298.0",
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.12",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "aws-sdk-client-mock": "^2.1.1",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.0",
    "module-alias": "^2.2.2",
    "prettier": "^2.8.7",
    "semantic-release": "^20.1.3",
    "serverless": "^3.29.0",
    "serverless-jetpack": "^0.11.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.4",
    "supertest": "^6.1.6",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "^9.3.12",
    "@nestjs/platform-express": "^9.3.12",
    "@streamamg/cloudpay-api-common-error-handler": "^1.2.0",
    "@streamamg/cloudpay-api-common-site-context": "^2.0.0",
    "aws-sdk": "^2.1376.0",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "got": "^12.6.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.0"
  }
}
